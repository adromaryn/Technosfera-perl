% layout 'default';
% title 'Image';

<h1>Add image</h1>

%= form_for "/tracks/id" . $id . '/image' =>(method=>'POST') => (enctype=>"multipart/form-data")=> begin
  %= csrf_field
  %= file_field "image", id => "image"
  <br /><br />
  %= submit_button "Upload", id => "submit", class => "btn btn-primary"
%= end
%= link_to 'Back', "/tracks/id$id/edit";
